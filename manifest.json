{
  "manifest_version": 2,

  "name": "Twenty Thousand Leagues New Tab",
  "description": "",
  "version": "0.1.1",

  "browser_action": {
    "default_icon": "icon.png"
  },

  "chrome_url_overrides" : {
    "newtab": "index.html"
  },

  "permissions": [
    "topSites",
    "bookmarks"
  ],

  "content_security_policy": "script-src 'self' https://unpkg.com/vue/dist/vue.js; object-src 'self'"
}